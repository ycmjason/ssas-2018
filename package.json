{
  "name": "ssas-2018",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "firebase -P default deploy",
    "predeploy:dev": "npm run build -- --mode development",
    "deploy:dev": "firebase -P dev deploy",
    "preversion": "$EDITOR CHANGELOG.md",
    "version": "git add -A",
    "prettier": "prettier --write 'src/**/*.@(js|vue)'"
  },
  "dependencies": {
    "firebase": "^6.6.1",
    "normalize.css": "^8.0.1",
    "prettier": "^1.18.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuefire": "^2.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@ycm.jason/eslint-config": "^1.3.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-plugin-vue": "^5.2.3",
    "firebase-tools": "^7.3.2",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
